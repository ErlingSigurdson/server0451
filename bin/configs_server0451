// Current (last posted) load status command for this instance of server0451.
CURRENT_CMD=1
// Plain ASCII command for turning load ON.
ASCII_CMD_ON=AT+SETLOAD=ON
// Command for turning load ON found in GET request URI.
URI_CMD_ON=/setloadon
// Command for turning load ON found in POST request key+value pair.
VAL_CMD_ON=cmd=setloadon
// Plain ASCII command for turning load OFF.
ASCII_CMD_OFF=AT+SETLOAD=OFF
// Command for turning load OFF found in GET request URI.
URI_CMD_OFF=/setloadoff
// Command for turning load OFF found in POST request key+value pair.
VAL_CMD_OFF=cmd=setloadoff
// Plain ASCII command for toggling load.
ASCII_CMD_TOGGLE=AT+SETLOAD=TOGGLE
// Command for toggling load found in GET request URI.
URI_CMD_TOGGLE=/setloadtoggle
// Command for toggling load found in POST request key+value pair.
VAL_CMD_TOGGLE=cmd=setloadtoggle
// Plain ASCII request for a current (last posted) load status command.
REQUEST_CMD=GETLOAD
